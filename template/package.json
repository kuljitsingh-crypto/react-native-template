{
  "name": "ReactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-win:clean": "cd android; ./gradlew clean; cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios:clean:": "cd ios && xcodebuild clean && cd ..",
    "native:android": "npx react-native run-android",
    "native:ios": "npx react-native run-ios",
    "android": "npm-run-all -p  native:android start",
    "ios": "npm-run-all -p native:ios start",
    "android-clean-run": "npm-run-all -s android:clean native:android",
    "ios-clean-run": "npm-run-all -s ios:clean native:ios",
    "dev-android-win": "set NODE_ENV= development && npm-run-all -s android",
    "dev-android-win:clean": "npm-run-all -s android-win:clean android",
    "dev-ios": "npm-run-all -s ios:clean ios",
    "dev-android": "npm-run-all -s android",
    "dev-android:clean": "npm-run-all -s android:clean android",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-firebase/app": "^20.4.0",
    "@react-native-firebase/messaging": "^20.4.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.6",
    "expo": "^51.0.32",
    "i18next": "^23.11.5",
    "native-form": "^2.1.3",
    "react": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-native": "^0.75.2",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-permissions": "^4.1.5",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.3.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@types/react": "^18.2.6",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
